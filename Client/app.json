{
  "expo": {
    "name": "PlantPal",
    "slug": "PlantPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/welcomeImg.png",
    "scheme": "plantpal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/welcomeImg.png",
      "supportsTablet": true,
      "usesAppleSignIn": false,
      "bundleIdentifier": "com.mohamed.plantpal",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/welcomeImg.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/welcomeImg.png"
      },
      "package": "com.mohamed.plantpal",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/welcomeImg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/welcomeImg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9cdabf9f-c25f-44da-b6c6-c6f20362968a"
      }
    }
  }
}